{
  "parser": "@babel/eslint-parser",
  "env": { "es6": true, "browser": true, "node": true },
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "no-console": "off"
  },
  "extends": ["eslint:recommended", "airbnb-base/legacy"],
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": "typescript"
  }
}
